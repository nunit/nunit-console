<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>nunit-console-runner-v4</id>
    <version>0.0.0</version>
    <title>NUnit Console Runner V4</title>
    <summary>Console runner for the NUnit unit-testing framework.</summary>
    <description>
      This package includes version 4 of the NUnit Console Runner and test engine.
    </description>
    <projectUrl>https://nunit.org</projectUrl>
    <projectSourceUrl>https://github.com/nunit/nunit-console</projectSourceUrl>
    <docsUrl>https://github.com/nunit/docs/wiki/Console-Command-Line</docsUrl>
    <bugTrackerUrl>https://github.com/nunit/nunit-console/issues</bugTrackerUrl>
    <iconUrl>https://cdn.rawgit.com/nunit/resources/master/images/icon/nunit_256.png</iconUrl>
    <packageSourceUrl>https://github.com/nunit/nunit-console</packageSourceUrl>
    <releaseNotes>https://docs.nunit.org/articles/nunit/release-notes/console-and-engine.html</releaseNotes>
    <mailingListUrl>https://groups.google.com/forum/#!forum/nunit-discuss</mailingListUrl>
    <licenseUrl>https://raw.githubusercontent.com/nunit/nunit-console/master/LICENSE.txt</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <authors>Charlie Poole, Rob Prouse</authors>
    <owners>Charlie Poole, Rob Prouse</owners>
    <language>en-US</language>
    <tags>nunit console runner test testing tdd</tags>
    <copyright>Copyright (c) 2021 Charlie Poole, Rob Prouse</copyright>
    <dependencies>
      <dependency id="nunit-extension-net462-pluggable-agent" version="4.0.0-alpha.6" />
      <dependency id="nunit-extension-net80-pluggable-agent" version="4.0.0-alpha.4" />
    </dependencies>
  </metadata>
  <files>
    <file src = "../LICENSE.txt" target = "tools" />
    <file src = "../NOTICES.txt" target = "tools" />
    <file src = "VERIFICATION.txt" target = "tools" />
    <file src = "nunit.choco.addins" target = "tools" />

    <file src = "$BIN_DIR$net462/nunit-console.exe" target="tools" />
    <file src = "$BIN_DIR$net462/nunit-console.exe.config" target="tools" />
    <file src = "$BIN_DIR$net462/nunit.engine.api.dll" target="tools" />
    <file src = "$BIN_DIR$net462/nunit.engine.api.xml" target="tools" />
    <file src = "$BIN_DIR$net462/nunit.extensibility.dll" target="tools" />
    <file src = "$BIN_DIR$net462/nunit.extensibility.api.dll" target="tools" />
    <file src = "$BIN_DIR$net462/nunit.common.dll" target="tools" />
    <file src = "$BIN_DIR$net462/nunit.engine.dll" target="tools" />
    <file src = "$BIN_DIR$net462/testcentric.metadata.dll" target="tools" />
  </files>
</package>
